  conteudo5  String? @db.VarChar(255)
  avisos6    String? @db.VarChar(255)
  conteudo6  String? @db.VarChar(255)
  avisos7    String? @db.VarChar(255)
  conteudo7  String? @db.VarChar(255)
  avisos8    String? @db.VarChar(255)
  conteudo8  String? @db.VarChar(255)
  avisos9    String? @db.VarChar(255)
  conteudo9  String? @db.VarChar(255)
  avisos10   String? @db.VarChar(255)
